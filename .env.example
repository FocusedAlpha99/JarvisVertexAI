# Vertex AI Configuration
VERTEX_PROJECT_ID=your-project-id
VERTEX_REGION=us-central1
VERTEX_AUDIO_ENDPOINT=projects/[PROJECT]/locations/[REGION]/endpoints/[ENDPOINT_ID]
VERTEX_CMEK_KEY=projects/[PROJECT]/locations/[REGION]/keyRings/vertex-ai-keyring/cryptoKeys/vertex-ai-cmek

# Google OAuth Configuration (2025 Enhanced)
GOOGLE_OAUTH_CLIENT_ID=your-client-id.apps.googleusercontent.com

# Gemini API Configuration (Required for all modes)
GEMINI_API_KEY=your-gemini-api-key

# Privacy Settings (DO NOT CHANGE)
VERTEX_AI_EXPLICIT_CACHE_MODE=off
DISABLE_PROMPT_LOGGING=true
DATA_RETENTION=NONE
PHI_REDACTION_ENABLED=true
LOCAL_ONLY_MODE=true
CLOUD_SYNC_ENABLED=false

# Optional: EU Data Residency (for GDPR)
# VERTEX_REGION=europe-west4
# DATA_RESIDENCY_REGION=EU