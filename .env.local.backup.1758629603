# JarvisVertexAI Environment Configuration
# Copy this file to .env.local and configure with your actual values

# Vertex AI Project Configuration
VERTEX_PROJECT_ID=your-project-id
VERTEX_REGION=us-central1
VERTEX_CMEK_KEY=projects/[PROJECT]/locations/[REGION]/keyRings/vertex-ai-keyring/cryptoKeys/vertex-ai-cmek

# Google OAuth Configuration
GOOGLE_OAUTH_CLIENT_ID=your-client-id.apps.googleusercontent.com

# Privacy Configuration
VERTEX_AI_EXPLICIT_CACHE_MODE=off

# Audio Endpoint (for Mode 1)
VERTEX_AUDIO_ENDPOINT=gemini-live-audio

# Optional: Access Token (for development only)
# VERTEX_ACCESS_TOKEN=your-access-token

# Privacy Flags (DO NOT CHANGE - Required for compliance)
DISABLE_PROMPT_LOGGING=true
DISABLE_DATA_RETENTION=true
DISABLE_MODEL_TRAINING=true
ENABLE_PHI_REDACTION=true
LOCAL_ONLY_MODE=true
ZERO_RETENTION_MODE=true

# File Retention (hours)
EPHEMERAL_FILE_RETENTION=24

# Database Configuration
DB_ENCRYPTION_ENABLED=true
DB_CLOUD_SYNC_DISABLED=true
AUTO_CLEANUP_DAYS=30